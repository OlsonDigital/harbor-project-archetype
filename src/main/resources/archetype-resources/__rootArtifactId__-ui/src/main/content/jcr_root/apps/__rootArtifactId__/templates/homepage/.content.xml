<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
          xmlns:icf="http://www.icfolson.com/ns/aem/"
          jcr:primaryType="cq:Template"
          jcr:title="${clientPrettyName} - Home Page"
          allowedPaths="/content(/.*)?"
          ranking="0">
    <jcr:content
            jcr:primaryType="cq:PageContent"
            jcr:mixinTypes="[icf:HomePage]"
            cq:designPath="/etc/designs/${rootArtifactId}"
            sling:resourceType="${rootArtifactId}/components/page/homepage"
            rdf:type="icf:HomePage"/>
</jcr:root>
